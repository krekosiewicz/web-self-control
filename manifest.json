{
  "manifest_version": 3,
  "name": "Page Bucket Organizer",
  "version": "1.0",
  "description": "Organize pages into different buckets.",
  "permissions": ["storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "bucket-icon.svg",
      "48": "bucket-icon.svg",
      "128": "bucket-icon.svg"
    }
  },
  "icons": {
    "16": "bucket-icon.svg",
    "48": "bucket-icon.svg",
    "128": "bucket-icon.svg"
  }
}
